uint
    x=123456789,
    y=362436069,
    z=521288629,
    w=88675123,
    v=886756453;
/* replace defaults with five random seed values in calling program */

uint xorshift(void)
{
    uint t = x^(x>>7);
    x=y; y=z; z=w; w=v;
    v=(v^(v<<6))^(t^(t<<13)); 
    return (y+y+1)*v;
} 

void seed(uint r)
{
	x = r;
	v = xorshift();
	w = xorshift();
	z = xorshift();
	y = xorshift();
}

